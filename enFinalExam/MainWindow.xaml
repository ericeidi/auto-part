        <Label x:Name="batteryTitleLbl"  Visibility="Hidden" Content="Battery" HorizontalAlignment="Left" Margin="196,133,0,0" VerticalAlignment="Top" Width="134" Height="47" VerticalContentAlignment="Center" Padding="5,5,4,5" HorizontalContentAlignment="Center" FontSize="24"/>
