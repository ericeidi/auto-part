        <Label x:Name="tireTitleLbl" Visibility="Hidden" Content="Tire" HorizontalAlignment="Left" Margin="196,133,0,0" VerticalAlignment="Top" Width="134" Height="47" VerticalContentAlignment="Center" Padding="5,5,4,5" HorizontalContentAlignment="Center" FontSize="24"/>
        <Label x:Name="quantityLbl" Visibility="Hidden" Content="Quantity" HorizontalAlignment="Left" Margin="48,239,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="quantityTxt" Visibility="Hidden" HorizontalAlignment="Left" Height="23" Margin="134,0,0,156" TextWrapping="Wrap" Text="" VerticalAlignment="Bottom" Width="120"/>

        <TextBox x:Name="itemNameBatteryTxt"  Visibility="Hidden"  HorizontalAlignment="Left" Height="23" Margin="134,201,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="voltageNameTxt"  Visibility="Hidden" HorizontalAlignment="Left" Height="23" Margin="133,318,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" TextChanged="diameterName_TextChanged"/>
